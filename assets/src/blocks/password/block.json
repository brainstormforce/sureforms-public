{
	"name": "sureforms/password",
	"title": "Password",
	"description": "Displays a SureForm Password Field",
	"category": "sureforms",
	"keywords": [ "password", "secure" ],
	"textdomain": "sureforms",
	"supports": {
		"reusable": false,
		"html": false
	},
	"example": {
		"attributes": {
			"preview": true
		}
	},
	"attributes": {
		"preview": {
			"type": "boolean",
			"default": false
		},
		"block_id": {
			"type": "string"
		},
		"required": {
			"type": "boolean",
			"default": false
		},
		"placeholder": {
			"type": "string",
			"default": ""
		},
		"label": {
			"type": "string",
			"default": "Password"
		},
		"help": {
			"type": "string",
			"default": ""
		},
		"errorMsg": {
			"type": "string",
			"default": "This field is required."
		},
		"confirmLabel": {
			"type": "string",
			"default": "Confirm Password"
		},
		"isConfirmPassword": {
			"type": "boolean",
			"default": false
		},
		"formId": {
			"type": "number",
			"default": ""
		}
	}
}
