{
	"name": "sureforms",
	"version": "0.0.1",
	"description": "Forms Plugin",
	"main": "index.js",
	"scripts": {
		"start": "wp-scripts start",
		"build": "wp-scripts build",
		"i18n": "grunt textdomain && wp i18n make-pot languages/sureforms.pot --domain=sureforms --debug",
		"pretty": "prettier --check .",
		"pretty:fix": "prettier --write .",
		"lint-js": "wp-scripts lint-js",
		"lint-js:fix": "npm run pretty:fix && npm run lint-js -- --fix",
		"lint-css": "wp-scripts lint-style --allow-empty-input",
		"lint-css:fix": "npm run lint-css -- --fix",
		"lint-pkg-json": "wp-scripts lint-pkg-json",
		"packages-update": "wp-scripts packages-update",
		"check-engines": "wp-scripts check-engines",
		"check-licenses": "wp-scripts check-licenses",
		"format": "wp-scripts format",
		"lint:md:docs": "wp-scripts lint-md-docs",
		"lint:md:js": "wp-scripts lint-md-js",
		"lint:pkg-json": "wp-scripts lint-pkg-json",
		"plugin-zip": "wp-scripts plugin-zip",
		"test:e2e": "wp-scripts test-e2e",
		"test:unit": "wp-scripts test-unit-js"
	},
	"repository": "git+https://github.com/brainstormforce/sureforms.git",
	"author": "Brainstorm Force",
	"license": "GPL",
	"bugs": {
		"url": "https://github.com/brainstormforce/sureforms/issues"
	},
	"homepage": "https://github.com/brainstormforce/sureforms#readme",
	"devDependencies": {
		"@emotion/core": "^11.0.0",
		"@emotion/react": "^11.10.6",
		"@wordpress/scripts": "^25.4.0",
		"grunt": "^1.4.1",
		"grunt-cli": "^1.4.3",
		"grunt-contrib-clean": "^2.0.0",
		"grunt-contrib-compress": "^2.0.0",
		"grunt-contrib-copy": "^1.0.0",
		"tailwindcss": "^3.2.7",
		"wp-prettier": "^2.6.2"
	},
	"private": true,
	"dependencies": {
		"@surecart/components": "^0.0.9",
		"@surecart/components-react": "^0.0.6",
		"@wordpress/icons": "^9.18.0",
		"lodash": "^4.17.21",
		"react-beautiful-dnd": "^13.1.1",
		"react-icons": "^4.8.0",
		"react-select": "^5.7.2"
	},
	"volta": {
		"node": "18.15.0"
	}
}
