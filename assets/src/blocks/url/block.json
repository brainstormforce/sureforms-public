{
	"name": "sureforms/url",
	"title": "URL",
	"description": "Displays a SureForm URL Field",
	"category": "sureforms",
	"keywords": [ "url", "link", "website" ],
	"ancestor": [ "sureforms/form" ],
	"textdomain": "sureforms",
	"supports": {
		"reusable": false,
		"html": false
	},
	"attributes": {
		"block_id": {
			"type": "string"
		},
		"defaultValue": {
			"type": "string"
		},
		"required": {
			"type": "boolean",
			"default": false
		},
		"placeholder": {
			"type": "string",
			"default": ""
		},
		"fieldWidth": {
			"type": "number",
			"default": 100
		},
		"label": {
			"type": "string",
			"default": "URL"
		},
		"help": {
			"type": "string",
			"default": ""
		},
		"errorMsg": {
			"type": "string",
			"default": "This field is required."
		},
		"formId": {
			"type": "number",
			"default": ""
		}
	}
}
