{
	"$schema": "https://json.schemastore.org/block.json",
	"apiVersion": 2,
	"name": "sureforms/sf-form",
	"title": "Form",
	"description": "Collect and Manage data effortlessly with customizable SureForm.",
	"category": "sureforms",
	"keywords": [ "form", "sureforms", "form" ],
	"textdomain": "sureforms",
	"supports": {
		"reusable": false,
		"html": false,
		"transforms": false
	},
	"attributes": {
		"id": {
			"type": "number"
		},
		"title": {
			"type": "string"
		},
		"choices": {
			"type": "array"
		},
		"template": {
			"type": "string"
		},
		"choice_type": {
			"type": "string",
			"enum": [ "all", "checkbox", "radio" ],
			"default": "all"
		}
	}
}
