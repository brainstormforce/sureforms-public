{
	"name": "sureforms/submit",
	"title": "Submit Button",
	"description": "Displays a SureForm Submit Field",
	"category": "sureforms",
	"keywords": [ "submit", "button" ],
	"textdomain": "sureforms",
	"ancestor": [ "sureforms/form" ],
	"supports": {
		"reusable": false,
		"html": false,
		"multiple": false
	},
	"attributes": {
		"id": {
			"type": "number"
		},
		"text": {
			"type": "string",
			"default": "Submit"
		},
		"full": {
			"type": "boolean",
			"default": false
		},
		"buttonAlignment": {
			"type": "string",
			"default": "left"
		}
	}
}
