{
	"name": "sureforms/hidden",
	"title": "Hidden",
	"description": "Displays a SureForm hidden Field",
	"category": "sureforms",
	"keywords": [ "hidden" ],
	"textdomain": "sureforms",
	"supports": {
		"reusable": false,
		"html": false
	},
	"example": {
		"attributes": {
			"preview": true
		}
	},
	"attributes": {
		"preview": {
			"type": "boolean",
			"default": false
		},
		"block_id": {
			"type": "string"
		},
		"defaultValue": {
			"type": "string",
			"default": ""
		},
		"label": {
			"type": "string",
			"default": "Hidden"
		},
		"fieldWidth": {
			"type": "number",
			"default": 100
		}
	}
}
