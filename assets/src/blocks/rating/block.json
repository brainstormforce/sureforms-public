{
	"name": "sureforms/rating",
	"title": "Rating Field",
	"description": "Displays a SureForm Rating Field",
	"category": "sureforms",
	"ancestor": [ "sureforms/form" ],
	"keywords": [ "rating", "star rating", "review", "field" ],
	"textdomain": "sureforms",
	"supports": {
		"reusable": false,
		"html": false
	},
	"attributes": {
		"id": {
			"type": "string",
			"default": ""
		},
		"required": {
			"type": "boolean"
		},
		"label": {
			"type": "string",
			"default": "Rate Us"
		},
		"ratingBoxHelpText": {
			"type": "string",
			"default":""
		},
		"width": {
			"type": "string",
			"default": "fullWidth"
		},
		"iconColor": {
			"type": "string",
			"default": "#000"
		},
		"showNumbers": {
			"type": "boolean"
		},
		"iconShape": {
			"type": "string",
			"default": "star"
		},
		"maxValue": {
			"type": "number",
			"default": 5
		},
		"errorMsg": {
			"type": "string",
			"default": "This field is required."
		},
		"formId": {
			"type": "number",
			"default": ""
		}
	}
}
