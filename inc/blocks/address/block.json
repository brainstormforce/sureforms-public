{
	"name": "sureforms/address",
	"title": "Address Field",
	"description": "Displays a SureForm Address Field",
	"category": "sureforms",
	"ancestor": [ "sureforms/form" ],
	"keywords": [ "address", "country", "location", "field" ],
	"textdomain": "sureforms",
	"supports": {
		"reusable": false,
		"html": false
	},
	"attributes": {
		"block_id": {
			"type": "string"
		},
		"required": {
			"type": "boolean"
		},
		"fieldWidth": {
			"type": "number",
			"default": 100
		},
		"label": {
			"type": "string",
			"default": "Address"
		},
		"errorMsg": {
			"type": "string",
			"default": "This field is required."
		},
		"lineOnePlaceholder": {
			"type": "string",
			"default": "Address Line 1"
		},
		"lineTwoPlaceholder": {
			"type": "string",
			"default": "Address Line 2"
		},
		"cityPlaceholder": {
			"type": "string",
			"default": "City"
		},
		"statePlaceholder": {
			"type": "string",
			"default": "State / Province / Region"
		},
		"postalPlaceholder": {
			"type": "string",
			"default": "Postal Code"
		},
		"lineOneLabel": {
			"type": "string",
			"default": "Address Line 1"
		},
		"lineTwoLabel": {
			"type": "string",
			"default": "Address Line 2"
		},
		"cityLabel": {
			"type": "string",
			"default": "City"
		},
		"stateLabel": {
			"type": "string",
			"default": "State / Province / Region"
		},
		"postalLabel": {
			"type": "string",
			"default": "Postal Code"
		},
		"countryLabel": {
			"type": "string",
			"default": "Country"
		},
		"countryPlaceholder": {
			"type": "string",
			"default": "Select Country"
		},
		"formId": {
			"type": "number",
			"default": ""
		},
		"help": {
			"type": "string",
			"default": ""
		}
	}
}
