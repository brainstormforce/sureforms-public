			"type": "string"
		},
		"defaultValue": {
			"type": "string"
		},
		"defaultCountryCode": {
			"type": "string"
		},
		"required": {
			"type": "boolean"
		},
		"fieldWidth": {
			"type": "number",
			"default": 100
		},
		"label": {
			"type": "string",
			"default": "Phone Number"
		},
		"placeholder": {
			"type": "string"
		},
		"help": {
			"type": "string",
			"default": ""
		},
		"isUnique": {
			"type": "boolean",
			"default": false
		},
		"duplicateMsg": {
			"type": "string",
			"default": "Value need to be unique."
		},
		"errorMsg": {
			"type": "string",
			"default": "This field is required."
