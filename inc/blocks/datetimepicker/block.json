{
	"name": "sureforms/date-time-picker",
	"title": "Date & Time Picker",
	"description": "Displays a SureForm Date & Time Field",
	"category": "sureforms",
	"ancestor": [ "sureforms/form" ],
	"keywords": [ "date", "time", "day", "date picker", "time picker" ],
	"textdomain": "sureforms",
	"supports": {
		"reusable": false,
		"html": false
	},
	"attributes": {
		"block_id": {
			"type": "string"
		},
		"fieldType": {
			"type": "string",
			"default": "dateTime"
		},
		"min": {
			"type": "string",
			"default": ""
		},
		"max": {
			"type": "string",
			"default": ""
		},
		"required": {
			"type": "boolean"
		},
		"fieldWidth": {
			"type": "number",
			"default": 100
		},
		"label": {
			"type": "string",
			"default": "Date & Time"
		},
		"help": {
			"type": "string",
			"default": ""
		},
		"errorMsg": {
			"type": "string",
			"default": "This field is required."
		},
		"formId": {
			"type": "number",
			"default": ""
		}
	}
}
