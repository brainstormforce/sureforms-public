{
	"apiVersion": 2,
	"name": "sureforms/multi-choice",
	"title": "Multiple Choice",
	"description": "Displays a SureForm Multi-Choice Field",
	"category": "sureforms",
	"ancestor": [ "sureforms/form" ],
	"keywords": [ "multi-choice", "quiz", "multi", "sureforms" ],
	"textdomain": "sureforms",
	"supports": {
		"reusable": false,
		"html": false
	},
	"attributes": {
		"block_id": {
			"type": "string"
		},
		"required": {
			"type": "boolean"
		},
		"singleSelection": {
			"type": "boolean"
		},
		"options": {
			"type": "array",
			"default": [
				{
					"optiontitle":"Option Name 1"
				}
			]
		},
		"label": {
			"default": "Multi Choice",
			"type": "string",
			"selector": "input"
		},
		"help": {
			"type": "string",
			"default": ""
		},
		"style": {
			"type": "string",
			"default": "buttons"
		},
		"errorMsg": {
			"type": "string",
			"default": "This field is required."
		},
		"formId": {
			"type": "number",
			"default": ""
		}
	}
}
