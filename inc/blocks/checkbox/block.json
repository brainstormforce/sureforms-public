{
	"apiVersion": 2,
	"name": "sureforms/checkbox",
	"title": "Checkbox",
	"description": "Displays a SureForm Checkbox Field",
	"category": "sureforms",
	"keywords": [ "checkbox", "toggle", "checkout", "engine" ],
	"textdomain": "sureforms",
	"supports": {
		"reusable": false,
		"html": false
	},
	"example": {
		"attributes": {
			"preview": true
		}
	},
	"attributes": {
		"preview": {
			"type": "boolean",
			"default": false
		},
		"block_id": {
			"type": "string"
		},
		"label": {
			"type": "string",
			"default": "Checkbox"
		},
		"labelUrl": {
			"type": "string",
			"default": ""
		},
		"required": {
			"type": "boolean"
		},
		"checked": {
			"type": "boolean",
			"default": false
		},
		"checkboxHelpText": {
			"type": "string",
			"default": ""
		},
		"errorMsg": {
			"type": "string",
			"default": "This field is required."
		},
		"formId": {
			"type": "number",
			"default": ""
		}
	}
}
