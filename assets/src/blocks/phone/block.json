{
	"name": "sureforms/phone",
	"title": "Phone Number",
	"description": "Displays a form Phone Number field. This is saved with the order as additional metadata.",
	"category": "sureforms",
	"ancestor": [ "sureforms/form" ],
	"keywords": [ "telephone", "phone", "mobile", "field" ],
	"textdomain": "sureforms",
	"supports": {
		"reusable": false,
		"html": false
	},
	"attributes": {
		"id": {
			"type": "string",
			"default": ""
		},
		"defaultValue": {
			"type": "string"
		},
		"defaultCountryCode": {
			"type": "string"
		},
		"required": {
			"type": "boolean"
		},
		"label": {
			"type": "string",
			"default": "Phone Number"
		},
		"placeholder": {
			"type": "string",
			"default": "9876543210"
		},
		"help": {
			"type": "string",
			"default": "Enter some help text here"
		}
		,
		"isUnique": {
			"type": "boolean",
			"default": false
		},
		"duplicateMsg": {
			"type": "string",
			"default": "Value need to be unique."
		},
		"errorMsg": {
			"type": "string",
			"default": "this field is required."
		}
	}
}
